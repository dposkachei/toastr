{
    "name": "bulk/toastr",
    "description": "Laravel toastr notification",
    "keywords": [
        "bulk",
        "toastr"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dposkachei/toastr",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Dmitrii Poskachei",
            "email": "dposkachei@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.4.5",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Bulk\\Toastr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bulk\\Toastr\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
